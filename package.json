{
  "name": "@henrist/cdk-cross-region-params",
  "version": "0.0.0-development",
  "description": "CDK Constructs for cross-region SSM Parameters and resources",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-cross-region-params"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cdk",
    "ssm parameters",
    "cross-region"
