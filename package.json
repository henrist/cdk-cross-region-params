  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.51.0",
    "@aws-cdk/aws-ssm": "1.51.0",
    "@aws-cdk/core": "1.51.0",
    "@aws-cdk/custom-resources": "1.51.0",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "aws-sdk": "2.713.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "jest-cdk-snapshot": "1.3.0",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1",
    "ts-jest": "26.1.2",
    "ts-node": "8.10.2",
    "typescript": "3.9.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-ssm": "^1.50.0",
    "@aws-cdk/core": "^1.50.0",
    "@aws-cdk/custom-resources": "^1.50.0"
  }
