{
  "name": "@henrist/cdk-cross-region-params",
  "version": "0.0.0-development",
  "description": "CDK Constructs for cross-region SSM Parameters and resources",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-cross-region-params"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cdk",
    "ssm parameters",
    "cross-region"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.77.0",
    "@aws-cdk/aws-ssm": "1.77.0",
    "@aws-cdk/core": "1.77.0",
    "@aws-cdk/custom-resources": "1.77.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/jest": "26.0.16",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "aws-sdk": "2.799.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-deprecation": "1.2.0",
    "eslint-plugin-prettier": "3.2.0",
    "husky": "4.3.5",
    "jest": "26.6.3",
    "jest-cdk-snapshot": "1.4.1",
    "prettier": "2.2.1",
    "semantic-release": "17.3.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-ssm": "^1.50.0",
    "@aws-cdk/core": "^1.50.0",
    "@aws-cdk/custom-resources": "^1.50.0"
  }
